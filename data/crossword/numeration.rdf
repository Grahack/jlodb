    <rdf:Description>
        <dct:identifier>n120</dct:identifier>
        <dct:title xml:lang="fr-FR">Trouver le nombre suivant (compris entre 10 et 20)</dct:title>
        <dct:description><![CDATA["keypad":"num0","hint":false,"horiz":true,"gen":"function(){var data=[],horiz={}; for (var i=0;i<8;i++){var a=Math.floor(Math.random()*8)+10; data.push((a+1).toString()); horiz['0x'+i]=a.toString();} return {\\\"data\\\":data,\\\"fixed\\\":[[0,0],[1,0]],\\\"def\\\":{\\\"horiz\\\":horiz}}}","font":4,"number":3]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":"Trouvez le nombre suivant..."]]></dct:description>
        <dct:extent>2</dct:extent>
