    <rdf:Description>
        <dct:identifier>abs1</dct:identifier>
        <dct:title xml:lang="fr-FR">Valeur absolue</dct:title>
        <dct:description><![CDATA["number":10,"data":[{"question":"Comment note-t'on, en mathématiques, la valeur absolue du nombre 99 ?", "good":["|99|"], "wrong":["(99)","'99'"]},{"question":"Que vaut |99| ?", "good":["99"], "wrong":["-99"]},{"question":"Que vaut |-99| ?", "good":["99"],"wrong":["-99"]},{"question":"Quel est la valeur de |-1| ?", "good":["1"],"wrong":["-1"]},{"question":"Quel est la valeur de |-1.1| ?", "good":["1.1"],"wrong":["-1.1","1","-1"]},{"question":"Que vaut |3+1| ? ", "good":["4"],"wrong":["31","-4", "2"]},{"question":"Que vaut |3-1| ?", "good":["2"],"wrong":["4","31","-4"]},{"question":"Que vaut |3|-|1| ?", "good":["2"],"wrong":["4","31","-4"]},{"question":"Que vaut |3|+|-1| ?", "good":["4"],"wrong":["2","-4","31"]},{"question":"Quelle est la valeur de |-3|-|-1| ?", "good":["2"],"wrong":["4","-4","31"]}],"class":"t0"]]></dct:description>
        <dct:extent>2</dct:extent>
