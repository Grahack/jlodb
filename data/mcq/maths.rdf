    <rdf:Description>
        <dct:identifier>div2</dct:identifier>
        <dct:title xml:lang="fr-FR">Critère de divisibilité par 2</dct:title>
        <dct:description><![CDATA["number":13,"data":[{"question":"Le nombre <b>16</b> est-il divisible par 2 ?", "good":["<b>oui</b>, car, 6, le chiffre des unités est pair"],"wrong":["<b>oui</b>, car le nombre est impair","<b>non</b>, car le nombre est pair","<b>non</b>, car le chiffre des unités est impair"],"font":0.8},{"question":"Le nombre <b>27</b> est-il divisible par 2 ?", "good":["<b>non</b>, car le chiffre des unités n'est pas 0,2,4,6 ou 8"],"wrong":["<b>oui</b>, car le chiffre des unités est pair","<b>oui</b>, car le nombre est impair","<b>oui</b>, car le chiffre des dizaines du nombre 27 est 2"],"font":0.8},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["4","18","10"],"wrong":["3","11","17"],"font":3},{"question":"Pour q'un nombre soit divisible par 2, il faut que...", "good":["le nombre se termine par un des chiffres 0,2,4,6 ou 8"], "wrong":["le chiffre des unités soit strictement égal à 2","la somme de ses chiffres soit divisible par 2","tous les chiffres du nombre, sans exception, soient pairs"],"font":0.8},{"question":"Le nombre <b>366</b> est-il divisible par 2 ?", "good":["<b>oui</b>, car le chiffre des unités est pair"],"wrong":["<b>oui</b>, car le nombre est impair","<b>non</b>, car le nombre est pair","<b>non</b>, car le chiffre des unités est impair"],"font":0.8},{"question":"Le nombre <b>3 234</b> est-il divisible par 2 ?", "good":["<b>oui</b>, car le chiffre des unités est pair"],"wrong":["<b>oui</b>, car le nombre est impair","<b>non</b>, car tous les chiffres du nombre sont impairs","<b>non</b>, car le chiffre des unités est impair."],"font":0.8},{"question":"Le nombre <b>2 579</b> est-il divisible par 2 ?", "good":["non"],"wrong":["oui"],"font":3},{"question":"Le nombre <b>10 944</b> est-il divisible par 2 ?", "good":["oui"],"wrong":["non"],"font":3},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["19 780","100 320","3 408"],"wrong":["3 799","17","2 311"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["2 190","26 712", "2 162","1 236"],"wrong":["28 179","111"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["1 956","5 798"],"wrong":["3 111","1 129","4 473","2 231"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 2 ?","good":["2 483"],"wrong":["286 178","8 282","61 928","52 340","1 234"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 2 ?","good":["25 709"],"wrong":["186","600","13 308","75 366","300 988"],"font":1.5}],"class":"up"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>arithmetic</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
    </rdf:Description>

