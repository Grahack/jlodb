    <rdf:Description>
        <dct:identifier>div9</dct:identifier>
        <dct:title xml:lang="fr-FR">Critère de divisibilité par 9</dct:title>
        <dct:description><![CDATA["number":13,"data":[{"question":"Le nombre <b>99</b> est-il divisible par 9 ?", "good":["<b>oui</b>, car la somme de ses chiffres <small>(9+9=18)</small> est divisible par 9"],"wrong":["<b>oui</b>, car le chiffre des unités est égal à 9","<b>non</b>, car la somme de ses chiffres <small>(9+9=18)</small> n'est pas divisible par 9","<b>non</b>, car le chiffre des unités n'est pas égal à 9"],"class":"small h2"},{"question":"Le nombre <b>19</b> est-il divisible par 9 ?", "good":["<b>non</b>, car la somme de ses chiffres <small>(1+9=10)</small> n'est pas divisible par 9"],"wrong":["<b>non</b>, car le nombre ne se termine pas par 0,3,6 ou 9","<b>oui</b>, car la somme de ses chiffres <small>(1+9=10)</small> est divisible par 9","<b>oui</b>, car le chiffre des unités est égal à 9."],"class":"small h2"},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 9","good":["27", "18","45"],"wrong":["19","21","33"]},{"question":"Pour q'un nombre soit divisible par 9, il faut que...", "good":["la somme de ses chiffres soit divisible par 9"], "wrong":["le nombre se termine par 0 ou 9","le chiffre des unités soit 9","la somme de ses chiffres soit égale à 9"],"class":"small h2"},{"question":"Le nombre <b>435</b> est-il divisible par 9 ?", "good":["<b>non</b>, car la somme de ses chiffres n'est pas divisible par 9"],"wrong":["<b>oui</b>, car la somme de ses chiffres est divisible par 9","<b>oui</b>, car le chiffre des unités est égal à 9","<b>non</b>, car le chiffre des unités n'est pas égal à 9"],"class":"small h2"},{"question":"Le nombre <b>5 859</b> est-il divisible par 9 ?", "good":["<b>oui</b>, car la somme de ses chiffres est divisible par 9"],"wrong":["<b>non</b>, car la somme de ses chiffres n'est pas divisible par 9","<b>non</b>, car la somme de ses chiffres n'est pas divisible par 9","<b>oui</b>, car la somme de ses chiffres est égale à 9."],"class":"small h2"},{"question":"Le nombre <b>6 718</b> est-il divisible par 9 ?", "good":["non"],"wrong":["oui"]},{"question":"Le nombre <b>27 331</b> est-il divisible par 9 ?", "good":["non"],"wrong":["oui"]},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 9","good":["27","90","4 896","333"],"wrong":["76","8 268"]},{"question":"Coche <b>tous</b> les nombres qui sont divisible par 9","good":["126","936","828"],"wrong":["13 025","218","17 776"]},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 9","good":["4 491","297"],"wrong":["590 622","38 933","551","550"]},{"question":"Quel est le nombre qui n'est pas divisible par 9 ?","good":["141"],"wrong":["4 959","99","63","5 229","171"]},{"question":"Quel est le nombre qui n'est pas divisible par 9 ?","good":["2 519"],"wrong":["63 297","216","45 846","307 062","18"]}],"class":"t0"]]></dct:description>
