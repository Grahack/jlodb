    <rdf:Description>
        <dct:identifier>dg100</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre l'écriture numérique et les notions de chiffres et de nombres</dct:title>
        <dct:description><![CDATA["number":10,"data":[{"question":"Un [b]chiffre[/b] est-il composé de [b]nombres[/b] ?", "good":["non"], "wrong":["oui"], "font":2},{"question":"Un [b]nombre[/b] est-il composé de [b]chiffres[/b] ?", "good":["oui"],"wrong":["non"], "font":2},{"question":"[b][red]123[/red][/b] est-il un [b]nombre[/b] ?", "good":["oui"],"wrong":["non"], "font":2},{"question":"[b][red]99[/red][/b] est-il un [b]chiffre[/b] ?", "good":["non"],"wrong":["oui"], "font":2},{"question":"[b][red]8[/red][/b] est-il un [b]nombre[/b] ?", "good":["oui"],"wrong":["non"], "font":2},{"question":"Quel est le [b]chiffre des dizaines[/b] du nombre [b][red]385[/red][/b] ?","good":["8"],"wrong":["3","5"], "font":2},{"question":"Quel est le [b]chiffre des unités[/b] du nombre [b][red]385[/red][/b] ?","good":["5"],"wrong":["8","3"], "font":2},{"question":"Quel est le [b]chiffre des centaines[/b] du nombre [b][red]385[/red][/b] ?","good":["3"],"wrong":["8","5"], "font":2},{"question":"[b]3[/b] est le chiffre des ... du nombre [b][red]783[/red][/b].","good":["unités"],"wrong":["milliers","dizaines","centaines"], "font":2},{"question":"[b]7[/b] est le chiffre des ... du nombre [b][red]783[/red][/b].","good":["centaines"],"wrong":["milliers","dizaines","unité"], "font":2}],"question":{"format":[2,93],"font":0.8}]]></dct:description>
