    <rdf:Description>
        <dct:identifier>dg1K</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre l'écriture numérique et les notions d'unités, de dizaines, de centaines et de milliers</dct:title>
        <dct:description><![CDATA["number":10,"data":[{"question":"<p>128 = 12 x 10 + 8</p><p>Combien de dizaines possède 128 ?</p>", "good":["12"], "wrong":["8","10","128","28","2"], "font":2},{"question":"<p>58 421 = 58 x 1000 + 421</p><p>Combien de milliers possède 58 421 ?</p>", "good":["58"], "wrong":["5","584","5 842"], "font":2},{"question":"<p>58 421 = 584 * 100 + 21</p><p>58 521 possède donc 584 ... ?</p>", "good":["Centaines"],"wrong":["Unités","Dizaines","Milliers"], "font":2},{"question":"<p>492 302 = 492 * 1000 + 302</p><p>492 302 possède donc 492 ... ?</p>", "good":["Milliers"],"wrong":["Unités","Dizaines","Centaines"], "font":2},{"question":"Quel est le <b>chiffre</b> des centaines dans 3 187 ?", "good":["1"],"wrong":["3","8","7"], "font":2},{"question":"Quel est le <b>nombre</b> de centaines dans 3 187 ?", "good":["31"],"wrong":["1","3","318"], "font":2},{"question":"Combien y-a-t'il de centaines dans 6 823 ?", "good":["68"],"wrong":["6","682","6 823"], "font":2},{"question":"Combien y-a-t'il de dizaines dans 6 823 ?", "good":["682"],"wrong":["6","68","6 823"], "font":2},{"question":"Combien y-a-t'il de milliers dans 6 823 ?", "good":["6"],"wrong":["68","682","6 823"], "font":2},{"question":"Dans 25 dizaines, quel est le nombre d'unités ?", "good":["250"],"wrong":["2","5","25"], "font":2}],"question":{"format":[2,93],"font":0.8}]]></dct:description>
        <dct:extent>2</dct:extent>
