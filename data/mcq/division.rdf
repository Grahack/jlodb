    <rdf:Description>
        <dct:identifier>c02</dct:identifier>
        <dct:title xml:lang="fr-FR">Appréhender les critères de divisibilité par 2.</dct:title>
        <dct:description><![CDATA["number":13,"data":[{"question":"<div class='q'>Un nombre entier est divisible par 2 lorsque son chiffre des unités est paire c'est à dire que le nombre finit par 0,2,4,6 ou 8.</div>","good":["Continuer"],"font":2},{"question":"Le nombre <b>16</b> est-il divisible par 2 ?", "good":["<b>OUI</b> car, 6, le chiffre des unités est pair."],"wrong":["<b>OUI</b> car le nombre est impair.","<b>NON</b> car le nombre est pair.","<b>NON</b> car le chiffre des unités est impair."],"font":0.8},{"question":"Le nombre <b>27</b> est-il divisible par 2 ?", "good":["<b>NON</b> car le chiffre des unités n'est pas 0,2,4,6 ou 8."],"wrong":["<b>OUI</b> car le chiffre des unités est pair.","<b>OUI</b> car le nombre est impair.","<b>OUI</b> car le chiffre des dizaines du nombre 27 est 2."],"font":0.8},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["4","18","10"],"wrong":["3","11","17"],"font":3},{"question":"Pour q'un nombre soit divisible par 2, il faut que...", "good":["le nombre se termine par un des chiffres 0,2,4,6 ou 8"], "wrong":["le chiffre des unités soit strictement égal à 2","la somme de ses chiffres soit divisible par 2","tous les chiffres du nombre, sans exception, soient pairs"],"font":0.8},{"question":"Le nombre <b>366</b> est-il divisible par 2 ?", "good":["<b>OUI</b> car le chiffre des unités est pair"],"wrong":["<b>OUI</b> car le nombre est impair","<b>NON</b> car le nombre est pair","<b>NON</b> car le chiffre des unités est impair"],"font":0.8},{"question":"Le nombre <b>3 234</b> est-il divisible par 2 ?", "good":["<b>OUI</b> car le chiffre des unités est pair"],"wrong":["<b>OUI</b> car le nombre est impair","<b>NON</b> car tous les chiffres du nombre sont impairs","<b>NON</b> car le chiffre des unités est impair."],"font":0.8},{"question":"Le nombre <b>2 579</b> est-il divisible par 2 ?", "good":["non"],"wrong":["oui"],"font":3},{"question":"Le nombre <b>10 944</b> est-il divisible par 2 ?", "good":["oui"],"wrong":["non"],"font":3},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["19 780","100 320","3 408"],"wrong":["3 799","17","2 311"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["2 190","26 712", "2 162","1 236"],"wrong":["28 179","111"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 2","good":["1 956","5 798"],"wrong":["3 111","1 129","4 473","2 231"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 2 ?","good":["2 483"],"wrong":["286 178","8 282","61 928","52 340","1 234"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 2 ?","good":["25 709"],"wrong":["186","600","13 308","75 366","300 988"],"font":1.5}],"class":"up"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>c02</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>c05</dct:identifier>
        <dct:title xml:lang="fr-FR">Appréhender les critères de divisibilité par 5.</dct:title>
        <dct:description><![CDATA["number":13,"data":[{"question":"<div class='q'>Un nombre entier est divisible par 5 lorsque son chiffre des unités est 0 ou 5.</div>","good":["Continuer"],"font":2},{"question":"Le nombre <b>11</b> est-il divisible par 5 ?", "good":["<b>NON</b> car le chiffre des unités est différent de 0 et de 5."],"wrong":["<b>NON</b> car le nombre ne commmence pas par 0 ou 5.","<b>OUI</b> car le nombre commence par 0 ou 5.","<b>OUI</b> car le chiffre des unités est 0 ou 5."],"font":0.8},{"question":"Le nombre <b>20</b> est-il divisible par 5 ?", "good":["<b>OUI</b> car le chiffre des unités du nombre 20 est 0."],"wrong":["<b>OUI</b> car tous les chiffres du nombre sont égaux à 0 ou 5.","<b>NON</b> car le chiffre des unités est différent de 0 et de 5.","<b>NON</b> car aucun chiffre du nombre 20 n'est égal à 0 ou 5."],"font":0.8},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 5","good":["10", "15", "20"],"wrong":["13","11","7"]},{"question":"Pour q'un nombre soit divisible par 5, il faut que...", "good":["le nombre se termine par 0 ou 5."], "wrong":["le chiffre des unités soit strictement égal à 5.","la somme de ses chiffres soit divisible par 5.","le chiffre des unités soit strictement égal à 0."],"font":0.8},{"question":"Le nombre <b>475</b> est-il divisible par 5 ?", "good":["<b>OUI</b> car le chiffre des unités est 5."],"wrong":["<b>OUI</b> car tous les chiffres du nombre sont égaux à 0 ou 5.","<b>NON</b> car le chiffre des unités est différent de 0 et de 5.","<b>NON</b> car aucun chiffre n'est égal à 0 ou 5."],"font":0.8},{"question":"Le nombre <b>2 179</b> est-il divisible par 5 ?", "good":["<b>NON</b> car le chiffre des unités est différent de 0 et de 5."],"wrong":["<b>NON</b> car le nombre ne commmence pas par 0 ou 5.","<b>OUI</b> car le nombre commence par 0 ou 5.","<b>OUI</b> car le chiffre des unités est 0 ou 5."],"font":0.8},{"question":"Le nombre <b>1 089</b> est-il divisible par 5 ?", "good":["non"],"wrong":["oui"],"font":3},{"question":"Le nombre <b>39 945</b> est-il divisible par 5 ?", "good":["oui"],"wrong":["non"],"font":3},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 5","good":["19 780","100 320"],"wrong":["3 408","3 799","17","2 311"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisible par 5","good":["2 190","6 915", "2 435"],"wrong":["1 236","28 179","111"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 5","good":["185", "102 515"],"wrong":["3 111","1 129","4 473","2 231"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 5 ?","good":["119 347"],"wrong":["380","423 690","1 035","48 590","185"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 5 ?","good":["54 998"],"wrong":["74 655","171 275","115","45","380"],"font":1.5}],"class":"up"]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>c02</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>c03a</dct:identifier>
        <dct:title xml:lang="fr-FR">Appréhender les critères de divisibilité par 3.</dct:title>
        <dct:description><![CDATA["number":13,"data":[{"question":"<div class='q'>Un nombre entier est divisible par 3 lorsque la somme de ses chiffres est divisible par 3.</div>","good":["Continuer"],"font":2},{"question":"Le nombre <b>18</b> est-il divisible par 3 ?", "good":["<b>OUI</b> car la somme de ses chiffres <small>(1+8=9)</small> est divisible par 3."],"wrong":["<b>OUI</b> car le chiffre des unités est égal à 3.","<b>NON</b> car la somme de ses chiffres n'est pas divisible par 3.","<b>NON</b> car le chiffre des unités n'est pas égal à 3."],"font":0.8},{"question":"Le nombre <b>11</b> est-il divisible par 3 ?", "good":["<b>NON</b> car la somme de ses chiffres <small>(1+1=2)</small> n'est pas divisible par 3."],"wrong":["<b>NON</b> car le nombre ne se termine pas par 0,3,6 ou 9.","<b>OUI</b> car la somme de ses chiffres est divisible par 3.","<b>OUI</b> car le chiffre des unités est égal à 3."],"font":0.8},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 3","good":["27", "15","12"],"wrong":["11","23","13"],"font":1.5},{"question":"Pour q'un nombre soit divisible par 3, il faut que...", "good":["la somme des chiffres soit divisible par 3."], "wrong":["le nombre se termine par 0,3,6 ou 9.","le chiffre des unités soit 3.","la somme de ses chiffres soit égale à 3."],"font":0.8},{"question":"Le nombre <b>927</b> est-il divisible par 3 ?", "good":["<b>OUI</b> car la somme de ses chiffres est divisible par 3."],"wrong":["<b>OUI</b> car le chiffre des unités est égal à 3.","<b>NON</b> car la somme de ses chiffres n'est pas divisible par 3.","<b>NON</b> car le chiffre des unités n'est pas égal à 3."],"font":0.8},{"question":"Le nombre <b>4 583</b> est-il divisible par 3 ?", "good":["<b>NON</b> car la somme de ses chiffres n'est pas divisible par 3."],"wrong":["<b>NON</b> car le nombre ne se termine pas par 0,3,6 ou 9.","<b>OUI</b> car la somme de ses chiffres est divisible par 3.","<b>OUI</b> car le chiffre des unités est égal à 3."],"font":0.8},{"question":"Le nombre <b>1 673</b> est-il divisible par 3 ?", "good":["non"],"wrong":["oui"],"font":3},{"question":"Le nombre <b>31 797</b> est-il divisible par 3 ?", "good":["oui"],"wrong":["non"],"font":3},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 3","good":["555","249 057","2 262","180","78 576"],"wrong":["131 093"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisible par 3","good":["492"],"wrong":["218","109","110","120 838","28 895"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 3","good":["42 717", "192 096","8 463","1 323"],"wrong":["10 036","619"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 3 ?","good":["1 853"],"wrong":["381","92 514","240","3 822","477 186"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 3 ?","good":["24 884"],"wrong":["14 808","9","78","103 428","14 160"],"font":1.5}],"class":"up"]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>3</dct:type>
        <dct:alternative>c02</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>c09</dct:identifier>
        <dct:title xml:lang="fr-FR">Appréhender les critères de divisibilité par 9.</dct:title>
        <dct:description><![CDATA["number":13,"data":[{"question":"<div class='q'>Un nombre entier est divisible par 9 lorsque la somme de ses chiffres est divisible par 9.</div>","good":["Continuer"],"font":2},{"question":"Le nombre <b>99</b> est-il divisible par 9 ?", "good":["<b>OUI</b> car la somme de ses chiffres <small>(9+9=18)</small> est divisible par 9."],"wrong":["<b>OUI</b> car le chiffre des unités est égal à 9.","<b>NON</b> car la somme de ses chiffres <small>(9+9=18)</small> n'est pas divisible par 9.","<b>NON</b> car le chiffre des unités n'est pas égal à 9."],"font":0.8},{"question":"Le nombre <b>19</b> est-il divisible par 9 ?", "good":["<b>NON</b> car la somme de ses chiffres <small>(1+9=10)</small> n'est pas divisible par 9."],"wrong":["<b>NON</b> car le nombre ne se termine pas par 0,3,6 ou 9.","<b>OUI</b> car la somme de ses chiffres <small>(1+9=10)</small> est divisible par 9.","<b>OUI</b> car le chiffre des unités est égal à 9."],"font":0.8},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 9","good":["27", "18","45"],"wrong":["19","21","33"],"font":1.5},{"question":"Pour q'un nombre soit divisible par 9, il faut que...", "good":["la somme de ses chiffres soit divisible par 9."], "wrong":["le nombre se termine par 0 ou 9.","le chiffre des unités soit 9.","la somme de ses chiffres soit égale à 9."],"font":0.8},{"question":"Le nombre <b>435</b> est-il divisible par 9 ?", "good":["<b>NON</b> car la somme de ses chiffres n'est pas divisible par 9."],"wrong":["<b>OUI</b> car la somme de ses chiffres est divisible par 9.","<b>OUI</b> car le chiffre des unités est égal à 9.","<b>NON</b> car le chiffre des unités n'est pas égal à 9."],"font":0.8},{"question":"Le nombre <b>5 859</b> est-il divisible par 9 ?", "good":["<b>OUI</b> car la somme de ses chiffres est divisible par 9."],"wrong":["<b>NON</b> car la somme de ses chiffres n'est pas divisible par 9.","<b>NON</b> car la somme de ses chiffres n'est pas divisible par 9.","<b>OUI</b> car la somme de ses chiffres est égale à 9."],"font":0.8},{"question":"Le nombre <b>6 718</b> est-il divisible par 9 ?", "good":["non"],"wrong":["oui"],"font":3},{"question":"Le nombre <b>27 331</b> est-il divisible par 9 ?", "good":["non"],"wrong":["oui"],"font":3},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 9","good":["27","90","4 896","333"],"wrong":["76","8 268"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisible par 9","good":["126","936","828"],"wrong":["13 025","218","17 776"],"font":1.5},{"question":"Coche <b>tous</b> les nombres qui sont divisibles par 9","good":["4 491","297"],"wrong":["590 622","38 933","551","550"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 9 ?","good":["141"],"wrong":["4 959","99","63","5 229","171"],"font":1.5},{"question":"Quel est le nombre qui n'est pas divisible par 9 ?","good":["2 519"],"wrong":["63 297","216","45 846","307 062","18"],"font":1.5}],"class":"up"]]></dct:description>
        <dct:extent>3</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
