    <!-- FORWARD,BACKWARD,PENCIL UP -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Faire avancer la tortue et tracer un segment vertical.</dct:title>
        <dct:description><![CDATA["a":{"op":["av"],"va":["V200"]},"bg":[{"id":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}}],"result":{"nb":1,"values":[[320,240,320,40,0]]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Tracez le segment de longeur 200.","","Depuis le menu 'source' en haut à gauche, faites glisser l'opérateur AVANCE vers la première ligne de la zone de programme un peu plus bas. Ensuite, cliquez sur l'onglet '1' du menu 'source' et faites glisser la valeur 200 de sorte à écrire 'AVANCE 200' dans la zone de programme.","","Lancez le programme ainsi créé. Si vous êtes satisfait du résultat, cliquez sur le bouton de validation pour terminer l'exercice."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>002</dct:identifier>
        <dct:title xml:lang="fr-FR">Tracer un segment vertical et cacher la tortue.</dct:title>
        <dct:description><![CDATA["a":{"op":["av","ct"],"va":["V200"]},"bg":[{"id":"line","attr":{"x1":320,"y1":40,"x2":320,"y2":240}}],"result":{"nb":1,"values":[[320,240,320,40,0]],"ct":false}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Tracez le segment de longeur 200 et cachez la tortue.","","L'instruction CACHE TORTUE permet de faire disparaître la tortue et ainsi de pouvoir mieux visualiser les éléments situés sous cette dernière.","","Sauf cas exceptionel comme celui-ci où l'opération 'CACHE TORTUE' est explicitement attendu, l'état de la tortue n'influe pas sur la notation finale."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutoriel</dct:coverage>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

