    <rdf:Description>
        <dct:identifier>A</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier le nombre 3 à partir d'une somme de 2 nombres (compris entre 1 et 2)</dct:title>
        <dct:description><![CDATA["number":4,"template":"array3x3","gen":"function(){var result=\\\"\\\",ret=[]; for (var i=0; i<9; i++) { var nb=2, a=Math.floor(Math.random()*nb)+1, b=Math.floor(Math.random()*nb)+1; result+=(a+b==3)?\\\"1\\\":\\\"0\\\"; ret.push(a+\\\"+\\\"+b);} return {result:result,data:ret}}","states":{ "nb":2, "css":[{"background-color":"rgba(255,255,255,0.8)","color":"gray"},{"background-color":"yellow","color":"orange"},{"background-color":"red","color":"pink"}]}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Coloriez en <span style='color:orange;'><b>JAUNE</b></span>, les additions dont la somme vaut <b>3</b>."]]></dct:description>
