    <rdf:Description>
        <dct:identifier>d1K2</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier, sans contrainte de temps, les nombres divisibles par 2 compris entre 1 et 1000.</dct:title>
        <dct:description><![CDATA["time":0,"template":"template.html","gen":"function() { var max=1000,v=2,r=Math.floor(Math.random()*2),a=Math.floor(Math.random()*max/v)*v+(r?0:Math.floor(Math.random()*(v-1))+1); return [a, (a%v)?0:1]}","font":2,"class":"h2"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"comment":"Le nombre est-il divisible par 2 ?","input":{"css":{"font-size":"3.0em","width":"3.0em"},"digit":1,"values":[["oui",1],["non",0]]}]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>division</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>d1K2</dct:alternative>
    </rdf:Description>

