    <rdf:Description>
        <dct:identifier>4</dct:identifier>
        <dct:title xml:lang="fr-FR">Critère de divisibilité par 3</dct:title>
        <dct:description><![CDATA["time":2,"len":10,"input":{"digit":1,"values":["oui","non"]},"template":"template.html","gen":"function() { var r=Math.floor(Math.random()*3), a=r?Math.floor(Math.random()*89999)+10001:(Math.floor(Math.random()*23332)+10001)*3; return [a, (a%3)?\\\"non\\\":\\\"oui\\\"]}","comment":"Le nombre est-il divisible par 3 ?","class":"h2"]]></dct:description>
        <dct:extent>1</dct:extent>
