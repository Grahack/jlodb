    <rdf:Description>
        <dct:identifier>5</dct:identifier>
        <dct:title xml:lang="fr-FR">Critère de divisibilité par 9</dct:title>
        <dct:description><![CDATA["time":1.6,"len":10,"input":{"digit":1,"values":["oui","non"]},"template":"template.html","gen":"function() { var r=Math.floor(Math.random()*2.1), a=r?Math.floor(Math.random()*999)+1:(Math.floor(Math.random()*110)+1)*9; return [a, (a%9)?\\\"non\\\":\\\"oui\\\"]}","comment":"Le nombre est-il divisible par 9 ?","class":"h2"]]></dct:description>
        <dct:extent>1</dct:extent>
