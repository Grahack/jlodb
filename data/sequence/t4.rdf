    <rdf:Description>
        <dct:identifier>6</dct:identifier>
        <dct:title xml:lang="fr-FR">Essais</dct:title>
        <dct:description><![CDATA["len":22,"input":{"digit":1,"values":["quel","quels","quelle","quelles","qu'elle","qu'elles"]},"regexp": { "output": "\\\\.\\\\.\\\\." },"template":"template.html","values":[["... chance pour ce gagnant !", "quelle"],["... sont ces fleurs ?","quelles"],["avec ... fruits fais-tu la tarte ?","quels"],["... beau feu d'artifice !","quel"],["pourvu ... se décide.","qu'elle"]],"class":"alpha h14"]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>languages</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>7</dct:identifier>
        <dct:title xml:lang="fr-FR">Les nombres entiers - vocabulaire</dct:title>
        <dct:description><![CDATA["time":1,"len":8,"template":"template.html","gen":"function() { var a,a1,a2,a3,b=Math.floor(Math.random()*3),ret=[]; do { a = Math.floor(Math.random()*900)+100; a1=Math.floor(a/100); a2=Math.floor(a/10)%10; a3=a%10; } while((a1==a2)||(a1==a3)||(a2==a3)); if (b==0 ) { ret=[a1+\\\" | \\\"+a, \\\"c\\\"]; } else if (b==1) { ret=[a2+\\\" | \\\"+a, \\\"d\\\"]; } else { ret=[a3+\\\" | \\\"+a, \\\"u\\\"]; } return ret;}","class":"alpha h2"]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"input":{"digit":1,"values":[["unité","u"],["dizaine","d"],["centaine","c"]]},"comment":"Quelle est la place du chiffre X dans le nombre Y ?"]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>mathematics</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>1</dct:type>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>8</dct:identifier>
        <dct:title xml:lang="fr-FR">Essais</dct:title>
        <dct:description><![CDATA["time":1.5,"len":10,"regexp":{"output": "\\\\?", "input": { "from": "([1-9]+) ([1-9]+) ([1-9]+)","to":"<math><mrow><mfrac><mrow><mi>$1</mi></mrow><mrow><mi>$2</mi></mrow></mfrac><mo>=</mo><mfrac><mrow><mi>?</mi></mrow><mrow><mi>$3</mi></mrow></mfrac></mrow></math>"}}, "input":{"digit":1},"values":[["1 2 4", "2"],["1 2 8","4"],["2 2 4","4"],["1 3 6","2"],["2 3 6","4"],["3 3 6","6"],["3 3 2","2"],["1 4 8","2"],["2 4 8","4"],["2 4 2","1"],["2 4 6","3"]],"class":"alpha h18"]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>mathematics</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>2</dct:type>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>9</dct:identifier>
        <dct:title xml:lang="fr-FR">Les ajectifs démonstratifs</dct:title>
        <dct:description><![CDATA["input":{"digit":1,"values":["ce","cet","cette","ces"]},"regexp": { "output": "\\\\.\\\\.\\\\." },"template":"template.html","values":[["... meuble", "ce"],["... téléviseur","ce"],[" ... télévision","cette"],["... chaise","cette"],["... placard","ce"],["... école","cette"],["... année","cette"],["... automobile","cette"],["... animal","cet"],["... hôpital","cet"],["... sport","ce"],["... ami","cet"],["... amie","cette"],["... enfants","ces"],["... hommes","ces"],["... femmes","ces"],["... chapeau","ce"],["... chaussette","cette"],["... chaussures","ces"],["... cravate","cette"],["... pantalon","ce"],["... arbre","cet"],["... maison","cette"],["... chemin","ce"],["... feuilles","ces"],["... accessoire","cet"],["... affaire","cette"],["... affaires","ces"],["... stylo","ce"],["... gomme","cette"],["... pot de colle","ce"],["... élève","cet"],["... étudiants","ces"],["... étudiantes","ces"],["... étudiante","cette"],["... apprenti","cet"],["... apprentis","ces"]],"class":"alpha h14 w2"]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>french</dct:subject>
        <dct:educationLevel>2</dct:educationLevel>
        <dct:type>2</dct:type>
        <dct:alternative>6</dct:alternative>
    </rdf:Description>


