    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Identifier un quadrilatère à partir du milieu de ses diagonales.</dct:title>
        <dct:description><![CDATA["data":[{"figure":{"content":"<path d='M 80,400 L 200,50 L 500,80 L 80,400 z' class='l'/><path d='M 200,50 L 290,240 L 380,430' class='l'/><circle cx='80' cy='400' r='5' class='p'/><circle cx='200' cy='50' r='5' class='p'/><circle cx='500' cy='80' r='5' class='p'/><circle cx='290' cy='240' r='5' class='p'/><circle cx='380' cy='430' r='5' class='p'/><text transform='translate(50,400)'>A</text><text transform='translate(170,50)'>C</text><text transform='translate(510,80)'>B</text><text transform='translate(285,220)'>M</text><text transform='translate(390,430)'>K</text>"},"values":[{"type":"tree","abstract":"#1","value":"middle","children":[{"type":"v","value":"M"},{"type":"v","subtype":"segment","value":"AB"}]},{"type":"tree","abstract":"#2","value":"middle","children":[{"type":"v","value":"M"},{"type":"v","subtype":"segment","value":"CK"}]}],"result":["parallelogram(ACBK)","parallelogram(CAKB)"]}],"class":"ex","glossary":true,"modif":false]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":[["Soit ABC un triangle, et [1]M le milieu de [AB][/1]. K est le point tel que [2]M est le milieu de [CK][/2].","","[b]Montrez que le quadrilatère ACBK est un parallélogramme.[/b]"]]]]></dct:description>
