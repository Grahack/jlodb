    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre un énoncé simple (2+3).</dct:title>
        <dct:description><![CDATA["values":[{"type":"v","value":2},{"type":"v","value":3},{"type":"op","value":"plus"}],"result":"2+3","class":"nofig","err":5]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Dans son panier, Léon a <b>3</b> pommes. Il décide maintenant d'achèter <b>2</b> poires.","","De combien de fruits Léon dispose-t'il désormais ? Représentez le résultat sous la forme d'une opération correspondant à l'énoncé."]]]></dct:description>
