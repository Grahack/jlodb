    <rdf:Description>
        <dct:identifier>004</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre un énoncé simple où les valeurs sont données littéralement (1+4+2).</dct:title>
        <dct:description><![CDATA["values":[{"type":"v","value":1},{"type":"v","value":2},{"type":"v","value":3},{"type":"v","value":4},{"type":"v","value":5},{"type":"op","value":"plus"}],"result":"1+4+2","class":"nofig","err":5]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Notre équipe est composée d'<b>un</b> chef de projets, de <b>quatre</b> développeurs et de <b>deux</b> testeurs.","","Combien de personnes travaillent dans notre équipe ?"]]]></dct:description>
