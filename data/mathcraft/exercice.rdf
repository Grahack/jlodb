    <rdf:Description>
        <dct:identifier>003</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre un énoncé simple (3+4+1).</dct:title>
        <dct:description><![CDATA["values":[{"type":"v","value":1},{"type":"v","value":2},{"type":"v","value":3},{"type":"v","value":4},{"type":"v","value":5},{"type":"op","value":"plus"}],"result":"3+4+1","class":"nofig","err":5]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Le train comporte <b>3</b> wagons de 1ère classe, <b>4</b> wagons de 2ème classe et d'<b>1</b> wagon restaurant.","","De combien de wagons est composé le train ?"]]]></dct:description>
