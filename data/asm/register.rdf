    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Comprendre le fonctionnement des registres.</dct:title>
        <dct:description><![CDATA["class":"large","ops":["lda"],"args":[{"value":"01","type":"v"}],"nblines":1,"valid":{"a":"01"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Un registre est une petite case du processeur qui peut contenir une valeur numérique entière comprise entre 0 et 255 (0x00 et 0xFF en hexadécimal).","","L'opérateur LDA stocke une valeur dans le registre A. Elle s'utilise avec la syntaxe suivante :","LDA [valeur]","","Chargez la valeur 0x01 dans le registre A.","A la fin de l'exécution du programme, le registre A doit contenir la valeur 0x01."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

