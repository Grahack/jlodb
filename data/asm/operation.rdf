    <!-- INX & INY -->

    <rdf:Description>
        <dct:identifier>001</dct:identifier>
        <dct:title xml:lang="fr-FR">Utiliser l'opération INX (1x) pour incrémenter la valeur du registre X.</dct:title>
        <dct:description><![CDATA["ops":["ldx","inx"],"args":[{"value":"A8","type":"v"}],"nblines":2,"valid":{"x":"A9"}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["L'opérateur INX s'utilise sans argument et permet d'incrémenter de 1 la valeur contenue dans le registre X.","","A l'aide des opérateurs disponibles, faites en sorte que le registre X contienne 0xA9 à la fin du programme."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

