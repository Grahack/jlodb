    <!-- INC & DEC -->

    <rdf:Description>
        <dct:identifier>151</dct:identifier>
        <dct:title xml:lang="fr-FR">Incrémenter une zone de la mémoire grâce à l'opérateur INC.</dct:title>
        <dct:description><![CDATA["class":"large","header":[{"label":"i","value":"","code":"ea","rts":false}],"ops":["lda","sta","inc"],"args":[{"value":"10","type":"v"},{"value":"i","type":"label"}],"nblines":3,"valid":{"mem":{"1000":"11"}}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["L'opérateur INC permet d'incrémenter la valeur d'un octet de la mémoire. Il s'utilise avec un argument : INC ADR où ADR peut être une adresse absolue (0x1000 par exemple) ou un label.","","Stockez la valeur 0x11 à l'adresse mémoire 0x1000 labellisé 'i' dans cet exercice."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
