    <rdf:Description>
        <dct:identifier>017</dct:identifier>
        <dct:title xml:lang="fr-FR">Convertir une chaîne de caractères en ROT13 en recopiant une partie de programme.</dct:title>
        <dct:description><![CDATA["class":"large","a":{"op":["print","ii","xx"],"va":["V000","VA","V001","V013","V026","I","X"],"ma":["plus","minus","len","get","modulo"],"cl":["rep"]},"values":{"TXT":"ABCMNOXYZ"},"valid":{"stdout":662546339}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Convertissez la chaîne de caractères TXT contenant le mot 'ABCMNOXYZ' en ROT13.","","Pour chaque lettre à la position J dans le mot TXT, la conversion se fait ainsi :",". X = GET(TXT,J) - 'A'",". X = X + 13",". X = X % 26",". X = X + 'A'","","Préférez l'écriture ci-dessus à PRINT ((GET(TXT,J)-'A')+13)%26+'A', plus court mais beaucoup moins lisible."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>001</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>018</dct:identifier>
