    <rdf:Description>
        <dct:identifier>007</dct:identifier>
        <dct:title xml:lang="fr-FR">Afficher la valeur de -PI suivie de l'entier inférieur le plus grand.</dct:title>
        <dct:description><![CDATA["class":"l1","a":{"op":["dump"],"va":["VPI"],"ma":["floor","neg"]},"valid":{"stdout":-724617246}]]></dct:description>
