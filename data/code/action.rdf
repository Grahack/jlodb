    <!-- DUMP VS PRINT -->

    <!-- PIXEL -->

    <rdf:Description>
        <dct:identifier>051</dct:identifier>
        <dct:title xml:lang="fr-FR">Afficher un pixel de l'écran graphique grâce à la commande PIXEL.</dct:title>
        <dct:description><![CDATA["class":"large","a":{"op":["pixel"],"va":["V000","V001","V002","V003","V004","V005","V006","V007","V008","V009","V010"]},"valid":{"screen":-763939344}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Coloriez en blanc le pixel en haut à gauche de l'écran graphique.","","La commande PIXEL(X,Y) permet de colorier le Xème pixel de la Yème ligne de l'écran graphique avec la couleur courrante. Par défault, cette couleur est blanche. Sachant qu'en informatique, les numérotations commencent la plupart du temps par 0, le pixel en haut à gauche de l'écran a (0,0) pour coordonnées."]]]></dct:description>
        <dct:extent>1</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutorial</dct:coverage>
        <dct:alternative>051</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>052</dct:identifier>
        <dct:title xml:lang="fr-FR">Coloriez les 4 coins de l'écran graphique grâce à la commande PIXEL.</dct:title>
        <dct:description><![CDATA["class":"large","a":{"op":["pixel"],"va":["V000","V001","V002","V003","V004","V005","V006","V007","V008","V009","V010","V030","V031","V032"]},"valid":{"screen":1640471626}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Coloriez en blanc les pixels des 4 coins de l'écran.","","Pour information, l'écran graphique de cette activité a une résolution de 32x32. N'oubliez pas que les numérotations commencent à 0, que, de fait, le 1er pixel de la 1ère ligne à (0,0) comme coordonnées. Quel est, à votre avis et dans ces conditions, les coordoonées du 32ème pixel de la 32ème ligne, par exemple ?","","Petite astuce pour gagner du temps : un paramètre non renseigné prend automatiquement la valeur 0."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>051</dct:alternative>
    </rdf:Description>

