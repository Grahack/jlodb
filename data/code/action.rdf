    <!-- RGB -->

    <rdf:Description>
        <dct:identifier>071</dct:identifier>
        <dct:title xml:lang="fr-FR">Peindre en rouge l'écran graphique grâce aux commandes RECT et RGB.</dct:title>
        <dct:description><![CDATA["class":"large","a":{"op":["rect","rgb"],"va":["V000","V001","V002","V003","V004","V005","V006","V007","V008","V009","V010","V016","V032","V255"]},"valid":{"screen":-859398104}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Coloriez en rouge l'ensemble de l'écran graphique.","","La commande RGB (les initiales de ROUGE,VERT,BLEU en anglais) permet de modifier la couleur courante en spécifiant chacune de ses 3 composantes. 0 signifie une absence totale de la tonalité (rouge, vert ou bleu), tandisque 255 signifie une présence maximale de ce ton.","","Quelques couleurs simples à connaître : noir = (0,0,0), blanc = (255,255,255), rouge = (255,0,0), vert = (0,255,0), bleu = (0,0,255), jaune = (255,255,0), violet = (255,0,255)"]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:coverage xml:lang="fr-FR">tutorial</dct:coverage>
        <dct:alternative>051</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>072</dct:identifier>
        <dct:title xml:lang="fr-FR">Coloriez les 4 coins de l'écran graphique en bleu grâce aux commandes PIXEL et RGB.</dct:title>
        <dct:description><![CDATA["class":"large","a":{"op":["pixel","rgb"],"va":["V000","V001","V002","V003","V004","V005","V006","V007","V008","V009","V010","V030","V031","V032","V255"]},"valid":{"screen":-1063194785}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Coloriez en bleu les pixels des 4 coins de l'écran.","","Pour rappel, RGB est l'accronyme de RED-GREEN-BLUE soit ROUGE-VET-BLEU en français. Chaque composante peut prendre une valeur comprise entre 0 (absence de la composante) à 255 (présence maximale). La couleur bleu est définie par une absence de rouge et de vert, et d'une présence maximale de bleu."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>051</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>073</dct:identifier>
        <dct:title xml:lang="fr-FR">Affichez un cadre vert grâce aux commandes RECT et RGB.</dct:title>
        <dct:description><![CDATA["class":"large","a":{"op":["rect","rgb"],"va":["V000","V004","V024","V032","V255"]},"valid":{"screen":2104596147}]]></dct:description>
        <dct:description xml:lang="fr-FR"><![CDATA[,"exercice":["Affichez un cadre vert de 4 pixels d'épaisseur remplissant l'écran.","","Dans un premier temps coloriez l'écran graphique en vert puis tracez un rectangle noir de dimension 24x24 centré au milieu de l'écran."]]]></dct:description>
        <dct:extent>2</dct:extent>
        <dct:subject>it</dct:subject>
        <dct:educationLevel>3</dct:educationLevel>
        <dct:type>1</dct:type>
        <dct:alternative>051</dct:alternative>
    </rdf:Description>

    <rdf:Description>
        <dct:identifier>074</dct:identifier>
